{
  "kind": "collectionType",
  "collectionName": "page",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true,
    "comment": ""
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    },
    "content-type-builder": {
      "visible": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "productIds": {
      "type": "customField",
      "customField": "plugin::medusa-product-selector.products"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "block.headings-block",
        "block.cta-image-block"
      ],
      "min": 1
    },
    "template": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::medusa-product-selector.template",
      "mappedBy": "page"
    }
  }
}
