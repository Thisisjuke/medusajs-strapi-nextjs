{
  "kind": "collectionType",
  "collectionName": "templates",
  "info": {
    "singularName": "template",
    "pluralName": "templates",
    "displayName": "Template"
  },
  "options": {
    "draftAndPublish": true,
    "comment": ""
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    },
    "content-type-builder": {
      "visible": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "block.cta-image-block",
        "block.headings-block"
      ],
      "required": true,
      "min": 1
    },
    "page": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::medusa-product-selector.page",
      "inversedBy": "template"
    }
  }
}
